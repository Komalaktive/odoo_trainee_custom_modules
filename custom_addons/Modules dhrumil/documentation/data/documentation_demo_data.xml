<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

    <!-- Demo data of docu.version model -->

        <record id="version_12" model="docu.version">
            <field name="version">12</field>
        </record>

        <record id="version_13" model="docu.version">
            <field name="version">13</field>
        </record>
        
        <record id="version_14" model="docu.version">
            <field name="version">14</field>
        </record>

        <record id="version_15" model="docu.version">
            <field name="version">15</field>
        </record>

    <!-- Demo data of docu.tags model -->

        <record id="tag_1" model="docu.tags">
            <field name="tag">Tag 1</field>
        </record>

        <record id="tag_2" model="docu.tags">
            <field name="tag">Tag 2</field>
        </record>

        <record id="tag_3" model="docu.tags">
            <field name="tag">Tag 3</field>
        </record>

        <record id="tag_4" model="docu.tags">
            <field name="tag">Tag 4</field>
        </record>

    <!-- Demo data of docu.item model -->

        <record id="item_1" model="docu.item">
            <field name="title">Sports</field>
            <field name="description_short">Short description about Sports</field>
            <field name="description_long">Long description about Sports</field>
            <field name="version" ref="version_13"/>
            <field name="tags" eval="[(6, 0, [ref('tag_1'),ref('tag_2')])]"/>
        </record>

        <record id="item_2" model="docu.item">
            <field name="title">Election</field>
            <field name="description_short">Short description about Election</field>
            <field name="description_long">Long description about Election</field>
            <field name="version" ref="version_12"/>
            <field name="tags" eval="[(6, 0, [ref('tag_2'),ref('tag_3')])]"/>
        </record>
        
        <record id="item_3" model="docu.item">
            <field name="title">Traffic</field>
            <field name="description_short">Short description about Traffic</field>
            <field name="description_long">Long description about Traffic</field>
            <field name="version" ref="version_14"/>
            <field name="tags" eval="[(6, 0, [ref('tag_3'),ref('tag_4')])]"/>
        </record>

    </data>
</odoo>
