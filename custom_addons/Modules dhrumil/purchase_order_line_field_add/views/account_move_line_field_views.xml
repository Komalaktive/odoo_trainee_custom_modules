<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_move_line_field_id" model="ir.ui.view">
        <field name="name">account.move.line.field.id</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@id='invoice_tab']/field[@name='invoice_line_ids']/tree/field[@name='product_id']" position="after">
                <field name="made_in"/>
            </xpath>
        </field>
    </record>

</odoo>