<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<template id="sale_report_inherit_template" inherit_id="sale.report_saleorder_document">
        
        <xpath expr="//th[@name='th_description']" position="after">
            <th>max qty allowed</th>
        </xpath>
        
        <xpath expr="//td[@name='td_name']" position="after">
            <td><span t-field="line.max_on_qty"></span></td>
        </xpath>

        <xpath expr="//div[@id='total']" position="after">
            <div style="float:right">
            <tr>
                <td><strong>Total Capicity</strong></td>
                    <span t-field="doc.total_capacity"/>
            </tr>
        </div>
        </xpath>

            

        <!-- <xpath expr="//div[@name='so_total_summary']/div/div/table[@class='table table-sm']/tr/td[@name='td_amount_untaxed_label']" position="after">
            <td name="td_label"><strong>Sub</strong></td>      
        </xpath>
 -->
    </template>
</odoo>