<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="contact_order_form_inherit" model="ir.ui.view">
        <field name="name">res.partner.inherited</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[4]" position="after">
                <page string="Team Leader" attrs="{'invisible': [('team_leader','==',False)]}">
                    <field name="team_ids" mode="tree">
                        <form>
                            <group>
                                <group>
                                    <field name="member_id" string="Name" options="{'no_open': True}"/>
                                    <field name="email"/>
                                    <field name="phone"/>
                                </group>
                                <group>
                                    <field name="address" readonly="1"/>
                                </group>
                            </group>
                        </form>
                    </field>
                </page>
            </xpath>
            <xpath expr="//field[@name='category_id']" position="after">
                <field name="team_leader"/>
            </xpath>
        </field>
    </record>
</odoo>