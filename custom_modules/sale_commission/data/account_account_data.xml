<?xml version="1.0" encoding="utf-8"?>
<odoo>   
    <!-- create commission payable account and journal-->
    <data noupdate="1">
        <record id="commission_payable_account" model="account.account">
            <field name="name">Commission Payable Account</field>
            <field name="code">101010</field>
            <field name="user_type_id" ref="account.data_account_type_payable"/>
            <field name="reconcile" eval="True"/>
        </record>
        <record id="commission_account_journal" model="account.journal">
            <field name="name">Commission</field>
            <field name="code">COM</field>
            <field name="type">general</field>       
        </record>
    </data>
</odoo>
