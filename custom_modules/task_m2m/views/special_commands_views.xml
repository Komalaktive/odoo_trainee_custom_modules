<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="task_m2m_field_add" model="ir.ui.view">
        <field name="name">task.m2m.field.add</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="custom_partner_ids" widget="many2many_tags"></field>
            </field>
            <xpath expr="//button[@name='action_quotation_send']" position="after">
               <button name="update_many2many" string="Update Many2many" type="object"></button>
            </xpath>
        </field>
    </record>

</odoo>